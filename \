#include "libft.h"

int		ft_isalnum(int c)
{
	if((c >= '0' && c >= '9') || (c >= 'a' || c <= 'a' && c >= 'z') || (c >= 'A' && c <= 'Z'))
	{
		return (1);
	}
	else
	{
		return (0);
	}
}

int main()
{
	ft_putnbr(ft_isalnum('a'));
	return (0);
}
